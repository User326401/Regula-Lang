import "std/io";

macro double(x: int) {
	x * 2;
};
macro PI 3.1415926;

func main() {
	let arr = [1, 2];
	for i in arr {
		io::println!(i);
	};

	for let i = 0; i < 100; i += 1 {
		if i % 2 == 0 {
		    io::println!(i);
		};
	};

	io::println!(add(1, 1));

	let minus = a, b => a - b;
	io::println!(minus(1, 1));

	io::println!(double!(PI));
};

func add(a: int, b: int) -> int {
	a + b;
};